                                                <li class="white-text font-12"><i class="fa fa-circle m-r-5 white-text text-accent-4 font-12"></i> Page Views </li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                                <div class="m-t-20" id="views"></div>
                            </div>
                        </div>
                    </div>



                </div>
                <!-- column -->
